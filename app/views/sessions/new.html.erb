
<% provide(:title, "Log in") %>





    <%= form_with(url: login_path, scope: :session, local: true, class: "form-signin") do |f| %>
      
      <%= f.label :email, 'email', class:"sr-only" %>
      <%= f.email_field :email, class: 'form-control', id:"inputEmail", class:"form-control", placeholder:"Email address" %>

      <%= f.label :password, 'senha', class:"sr-only"  %>
      <%= f.password_field :password, class: 'form-control', id:"inputPassword", class:"form-control", placeholder:"Password" %>
      
      <%= f.submit "Log in", class: "btn btn-lg btn-primary btn-block" %>
    <% end %>

    






