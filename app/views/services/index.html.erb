<div class="card">
  <div class="card-body">
    <table class="table table-striped">
      <thead>
        <tr>
          <th scope="col">Dia - Início do Serviço</th>
        </tr>
      </thead>
      <tbody>
        <% @services.each do |service| %>
        <tr>
          <td><%= link_to l(service.created_at, format: :day), service_path(service)%></td>
        </tr>
        <% end %>
      </tbody>
    </table>
    </div>
  </div>
  

  

  
      