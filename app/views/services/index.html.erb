<% @services.each do |service| %>
 <h2 class='text-center'>Serviço de <%= l(service.created_at, format: :day) %></h2>
  <table class="table table-striped">
    <thead>
      <tr>
        <th scope="col">Colaborador</th>
        <th scope="col">Chegada</th>
        <th scope="col">Saída</th>
        
        
      </tr>
    </thead>
    <tbody>
      <% service.jobs.each do |job| %>
      <tr>
        <td><%= job.user.name %></td>
        <td><%= l job.start_at, format: :short %></td>
        <td><%= job.end_at.nil? ? '' : l(job.start_at, format: :short)  %></td>
        
      </tr>
      <% end %>
    </tbody>
  </table>
<% end %>
  

  

  
      